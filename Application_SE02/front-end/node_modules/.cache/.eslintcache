age(age);\r\n          return { ...x, state: false };\r\n        } else {\r\n          if (age_num < age_limit) {\r\n            add_age(age);\r\n            return { ...x, state: true };\r\n          }\r\n          return x;\r\n        }\r\n      } else {\r\n        return x;\r\n      }\r\n    });\r\n\r\n    set_ages(new_ages);\r\n  }\r\n\r\n  /* --- Returned JSX object --- */\r\n  return (\r\n    <div>\r\n      <h2 className=\"heading\">4: Select an Age Range (Up to {age_limit})</h2>\r\n      <div className=\"age-button-container\">\r\n        {ages.map((element, index) => (\r\n          <label\r\n            key={index}\r\n            className={`age-button-${element.state ? \"on\" : \"off\"}`}\r\n          >\r\n            <input\r\n              className=\"age-button-checkbox\"\r\n              type=\"checkbox\"\r\n              onClick={() => {\r\n                handle_click(element.age);\r\n              }}\r\n            />\r\n            <p className=\"age-button-text\">\r\n              {element.age}\r\n              <br />\r\n            </p>\r\n            <p className=\"age-button-desc-text\">{element.desc}</p>\r\n          </label>\r\n        ))}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default SelectAge;\r\n",{"ruleId":"95","severity":1,"message":"96","line":39,"column":61,"nodeType":"97","messageId":"98","endLine":39,"endColumn":62},{"ruleId":"95","severity":1,"message":"96","line":39,"column":75,"nodeType":"97","messageId":"98","endLine":39,"endColumn":76},{"ruleId":"95","severity":1,"message":"96","line":30,"column":61,"nodeType":"97","messageId":"98","endLine":30,"endColumn":62},{"ruleId":"95","severity":1,"message":"96","line":30,"column":75,"nodeType":"97","messageId":"98","endLine":30,"endColumn":76},{"ruleId":"95","severity":1,"message":"96","line":29,"column":59,"nodeType":"97","messageId":"98","endLine":29,"endColumn":60},{"ruleId":"95","severity":1,"message":"96","line":29,"column":73,"nodeType":"97","messageId":"98","endL